                <stringProp name="filename"></stringProp>
                <stringProp name="parameters"></stringProp>
                <stringProp name="script">var k = vars.get(&quot;k&quot;);
var title = vars.get(&quot;title_&quot;+k).split(&quot; - &quot;)[1];
var link  = vars.get(&quot;link_&quot;+k);
var locations =  vars.get(&quot;location_&quot;+k);
var splitted = locations.split(&apos; \\\| &apos;);
var countries = [];
for (loc in splitted){
	var locsplit=loc.split(&quot;-&quot;);
	countries &lt;&lt; locsplit[0];
}
countries = countries as Set
for( country_code in countries)
{
	var country = &quot;&quot;
	var city = []
	switch(country_code){
		case &quot;RO&quot;:
			country = &quot;România&quot;
			break;
		case &quot;MD&quot;:
			country = &quot;Moldova&quot;
			break;
		case &quot;HU&quot;:
			country = &quot;Hungary&quot;
			break;
		default:
			log.warn(&quot;Unkown country: &quot;+conuntry_code)
			continue
		}
	log.info(country)
	for (loc in splitted){
		var locsplit=loc.split(&quot;-&quot;);
		if(locsplit[0]==country_code)
			city &lt;&lt; (&quot;\&quot;&quot; + locsplit[1] + &quot;\&quot;&quot;);
	}
	var id = UUID.randomUUID().toString();
	String this_job = &apos;{&quot;id&quot;:&quot;&apos;+id+&apos;&quot;,&quot;job_title&quot;:&quot;&apos;+title+&apos;&quot;, &quot;job_link&quot;:&quot;&apos;+link+&apos;&quot;,&quot;company&quot;:&quot;${company}&quot;,&quot;country&quot;:&quot;&apos;+country+&apos;&quot;,&quot;city&quot;:&apos;+city+&apos;}&apos;;
	var jobList = vars.getObject(&quot;jobList&quot;);
	jobList = jobList + this_job;
	vars.putObject(&quot;jobList&quot;,jobList);
}
</stringProp>
